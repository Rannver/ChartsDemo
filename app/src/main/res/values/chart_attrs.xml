<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LineChartView">


        <!--x,y轴颜色-->
        <attr name="xylinecolor" format="color"/>
        <!--x,y轴粗细-->
        <attr name="xylinewidth" format="dimension"/>
        <!--折线颜色-->
        <attr name="linecolor" format="color"/>
        <!--表格背景颜色-->
        <attr name="backroundcolor" format="color"/>
        <!--是否允许滑动-->
        <attr name="scrollable" format="boolean"/>
        <!--坐标点颜色-->
        <attr name="pointcolor" format="color"/>
        <!--填充区域颜色-->
        <attr name="fillcolor" format="color"/>
        <!--坐标轴文字大小-->
        <attr name="xytextsize" format="dimension"/>
        <!--坐标轴文字颜色-->
        <attr name="xytextcolor" format="color"/>
        <!--刻度长度-->
        <attr name="xyscalelength" format="dimension"/>
        <!--阈值设置-->
        <attr name="threshold" format="float"/>
        <!--是否显示阈值线-->
        <attr name="thresholdlineshow" format="boolean"/>
        
    </declare-styleable>

